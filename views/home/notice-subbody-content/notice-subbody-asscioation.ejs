<div class="notice-subbody">
    <div class=" notice-subbody-nav">
        <ul class="nav nav-pills nav-stacked nav-style">
            <li><img class="notice-nav-pic img-responsive" src="../images/nav-pic/notice-association.jpg"/></li>
            <li class="notice-nav-li" id="notice-nav1" role="presentation"><a class="notice-nav-font"
                                                                              href="/notice/notice-association">我会通告</a>
            </li>
            <li class="notice-nav-li" id="notice-nav2" role="presentation"><a class="notice-nav-font"
                                                                              href="/notice/notice-monitor">其他通告</a>
            </li>
        </ul>
    </div>
    <div class="notice-subbody-content">
        <div class="panel panel-border panel-primary">
            <div class="notice-rightbody-heading ">
                <ol class="breadcrumb breadcrumb-style">
                    <li><a href="/notice">通知公告</a></li>
                    <li><a href="/notice/notice-monitor">我会通告</a></li>
                </ol>
            </div>
            <div class="panel-body">
                <div class="notice-subbodybr">
                    <h2>
                        <ul>
                            <li class="nowb">我会通告</li>
                        </ul>
                    </h2>
                    <ol>
                        <% association.forEach(function (associpublish) { %>
                        <li><span><font><%= associpublish.associday %></font>/<%= associpublish.associmonth %></span><a
                                    href="/notice/<%= associpublish._id %>/notice-association-content" target="_blank"
                                    title="<%=associpublish.assocititle %>"><%=associpublish.assocititle %></a></li>
                        <% }) %>

                    </ol>
                </div>
            </div>
        </div>
    </div>
</div>