<div class="admin-subbody">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">协会动态</h3>
        </div>
        <div class="panel-body">
            <form id="defaultform" role="form" method="post" enctype="application/json" action="/admin/admin-dynamics/<%= associonedynamics._id %>/dassociupdate">
                <div class="form-group">
                    <label>协会动态名</label>
                    <input class="form-control" placeholder="请输入通知或公告名" type="text" name="dassocititle"
                           value=<%= associonedynamics.dassocititle %>>
                </div>

                <div class="form-group">
                    <label>月份</label>
                    <input class="form-control" placeholder="请输入月份" type="text" name="dassocimonth"
                           value=<%= associonedynamics.dassocimonth %>>
                </div>

                <div class="form-group">
                    <label>天数</label>
                    <input class="form-control" placeholder="请输入天数" type="text" name="dassociday"
                           value=<%= associonedynamics.dassociday %>>
                </div>

                <div class="form-group">
                    <label>监管动态内容</label>

                    <textarea id="container"  class="admin-modal-container" name="dassocibio"
                              type="text/plain"> <%- associonedynamics.dassocibio %></textarea>
                </div>
                <input type="submit" class="btn  btn-primary" value="修改">
            </form>
        </div>
        <!--<script type="text/javascript" src="../public/javascripts/update.js">-->
        <script type="text/javascript">

            var ue = UE.getEditor('container');
        </script>
    </div>
</div>
</div>
</div>
