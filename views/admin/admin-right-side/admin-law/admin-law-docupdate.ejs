<div class="admin-subbody">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">规范性文件</h3>
        </div>
        <div class="panel-body">
            <form id="defaultform" role="form" method="post" enctype="application/json" action="/admin/admin-law/<%= doconelaw._id %>/docupdate">
                <div class="form-group">
                    <label>规范性文件名</label>
                    <input class="form-control" placeholder="请输入通知或公告名" type="text" name="doctitle"
                           value=<%= doconelaw.doctitle %>>
                </div>

                <div class="form-group">
                    <label>月份</label>
                    <input class="form-control" placeholder="请输入月份" type="text" name="docmonth"
                           value=<%= doconelaw.docmonth %>>
                </div>

                <div class="form-group">
                    <label>天数</label>
                    <input class="form-control" placeholder="请输入天数" type="text" name="docday"
                           value=<%= doconelaw.docday %>>
                </div>

                <div class="form-group">
                    <label>规范性文件内容</label>

                    <textarea id="container"  class="admin-modal-container" name="docbio"
                              type="text/plain"> <%- doconelaw.docbio %></textarea>
                </div>
                <input type="submit" class="btn  btn-primary" value="修改">
            </form>
        </div>
        <!--<script type="text/javascript" src="../public/javascripts/update.js">-->
        <script type="text/javascript">
            $('#myModal').modal('show')
            var ue = UE.getEditor('container');
        </script>
    </div>
</div>
</div>
</div>
