<div class="admin-subbody">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">部门规章</h3>
        </div>
        <div class="panel-body">
            <div class="table-responsive">
                <table class="table table-bordered table-striped bs-events-table">
                    <thead>
                    <tr>
                        <th>日期</th>
                        <th>部门规章名</th>
                        <th>功能</th>
                    </tr>
                    </thead>
                    <% department1law.forEach(function (deplaw) { %>
                    <tbody>
                    <tr>
                        <td><%= deplaw.depday%>/<%= deplaw.depmonth%></td>
                        <td><%= deplaw.deptitle %></td>
                        <td>
                            <div class="item">
                                <a href="/admin/<%= deplaw._id %>/depremove"><span class="glyphicon glyphicon-trash color"/> </a>
                                <a href="/admin/admin-law/<%= deplaw._id %>/admin-law-depup" ><span class="glyphicon glyphicon-pencil"/> </a>
                            </div>
                        </td>
                    </tr>
                    </tbody>
                    <% }) %>
                </table>
            </div>
        </div>
    </div>
</div>
