<div class="admin-subbody">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">部门规章</h3>
        </div>
        <div class="panel-body">
            <div class="table-responsive">
                <table class="table table-bordered table-striped bs-events-table">
                    <thead>
                    <tr>
                        <th>教育</th>
                        <th>功能</th>
                    </tr>
                    </thead>
                    <% exam1education.forEach(function (exam) { %>
                    <tbody>
                    <tr>
                        <td><%= exam.etitle%></td>
                        <td>
                            <div class="item">
                                <a href="/admin/<%= exam._id %>/examremove"><span class="glyphicon glyphicon-trash" style="color: red"/> </a>
                                <a href="/admin/admin-education/<%= exam._id %>/admin-education-examup" ><span class="glyphicon glyphicon-pencil"/> </a>
                                <a href="/admin/admin-education/admin-education-exam" ><span class="glyphicon glyphicon-plus"/></a>
                            </div>
                        </td>
                    </tr>
                    </tbody>
                    <% }) %>
                </table>
            </div>
        </div>
    </div>
</div>
