<div class="admin-subbody">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">会员编辑</h3>
        </div>
        <div class="panel-body">
            <form id="defaultform" role="form" method="post" enctype="application/json" action="/admin/admin-members/<%= membersone._id %>/memupdate">
                <div class="form-group">
                    <label>会员名称</label>
                    <input class="form-control" type="text" name="membersname"
                           value=<%= membersone.membersname %>>
                </div>

                <div class="form-group">
                    <label>密码</label>
                    <input class="form-control"  type="text" name="memberspassword"
                           value=<%= membersone.memberspassword %>>
                </div>

                <input type="submit" class="btn  btn-primary" value="修改">
            </form>
        </div>

    </div>
</div>
</div>
</div>
