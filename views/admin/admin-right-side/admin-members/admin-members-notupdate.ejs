<div class="admin-subbody">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">法律法规</h3>
        </div>
        <div class="panel-body">
            <form id="defaultform" role="form" method="post" enctype="application/json" action="/admin/admin-members/<%= onenotice._id %>/notupdate">
                <div class="form-group">
                    <label>法律法规名</label>
                    <input class="form-control" placeholder="请输入通知或公告名" type="text" name="ntitle"
                           value=<%= onenotice.ntitle %>>
                </div>

                <div class="form-group">
                    <label>月份</label>
                    <input class="form-control" placeholder="请输入月份" type="text" name="nmonth"
                           value=<%= onenotice.nmonth %>>
                </div>

                <div class="form-group">
                    <label>天数</label>
                    <input class="form-control" placeholder="请输入天数" type="text" name="nday"
                           value=<%= onenotice.nday %>>
                </div>

                <div class="form-group">
                    <label>年</label>
                    <input class="form-control" placeholder="请输入年" type="text" name="nyear"
                           value=<%= onenotice.nyear %>>
                </div>

                <div class="form-group">
                    <label>法律法规内容</label>

                    <textarea id="container"  class="admin-modal-container" name="nbio"
                              type="text/plain"> <%- onenotice.nbio %></textarea>
                </div>
                <input type="submit" class="btn  btn-primary" value="修改">
            </form>
        </div>
        <!--<script type="text/javascript" src="../public/javascripts/update.js">-->
        <script type="text/javascript">
            var ue = UE.getEditor('container');
        </script>
    </div>
</div>
</div>
</div>
