<div class="admin-subbody">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">紧急通知</h3>
        </div>
        <div class="panel-body">
            <div class="table-responsive">
                <table class="table table-bordered table-striped bs-events-table">
                    <thead>
                    <tr>
                        <th>日期</th>
                        <th>紧急通知</th>
                        <th>功能</th>
                    </tr>
                    </thead>
                    <% allnotices.forEach(function (noticemem) { %>
                    <tbody>
                    <tr>
                        <td><%= noticemem.nday%>/<%= noticemem.nmonth%>/<%= noticemem.nyear%></td>
                        <td><%= noticemem.ntitle %></td>
                        <td>
                            <div class="item">
                                <a href="/admin/<%= noticemem._id %>/notremove"><span class="glyphicon glyphicon-trash" style="color:red" /></a>
                                <a href="/admin/admin-members/<%= noticemem._id %>/admin-members-notup" ><span class="glyphicon glyphicon-pencil"/> </a>
                                <a href="/admin/admin-members/admin-members-notice" ><span class="glyphicon glyphicon-plus"/> </a>
                            </div>
                        </td>
                    </tr>
                    </tbody>
                    <% }) %>
                </table>
            </div>
        </div>
    </div>
</div>
